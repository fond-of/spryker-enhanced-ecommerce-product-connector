{
  "name": "fond-of-spryker/enhanced-ecommerce-product-connector",
  "description": "provides product detail attributes for enhanced ecommerce",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "require": {
    "spryker/money": "^1.0 || ^2.0",
    "fond-of-spryker/enhanced-ecommerce-extension": "^1.0.0",
    "php": ">=7.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "fond-of-codeception/spryker": "dev-master as 1.0.0",
    "codeception/codeception": "^2.5",
    "mikey179/vfsstream": "^1.6",
    "php-coveralls/php-coveralls": "^2.1",
    "sebastian/phpcpd": "^4.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
